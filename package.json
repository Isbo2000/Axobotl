{
  "name": "axobotl",
  "version": "4.1.0",
  "description": "A multipurpose axolotl discord bot.",
  "main": "Index.js",
  "scripts": {
    "prepare": "husky install",
    "build": "rm -fr build; tsc && cp -r assets build",
    "start": "npm run build && node build/Index.js",
    "dev": "ts-node Index.ts",
    "format": "prettier --write"
  },
  "author": "Isbo2000",
  "license": "ISC",
  "dependencies": {
    "canvas": "^2.9.0",
    "discord.js": "^13.6.0",
    "markov-text-generator": "^1.0.2",
    "readline-sync": "^1.4.10"
  },
  "devDependencies": {
    "husky": "^7.0.4",
    "lint-staged": "^12.3.6",
    "prettier": "2.6.0",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.2"
  },
  "lint-staged": {
    "*.{js,css,md}": [
      "npm run format"
    ]
  }
}
